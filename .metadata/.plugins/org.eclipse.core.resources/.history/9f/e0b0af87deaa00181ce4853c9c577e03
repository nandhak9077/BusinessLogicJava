import java.util.Scanner;

public class Nine 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.print("Enter the value of n : ");
		int n = s.nextInt();
		int i=n;
		while(n>0)
		{
			System.out.print(n+" ");
			
			n--;
		}
		s.close();
	}
}
