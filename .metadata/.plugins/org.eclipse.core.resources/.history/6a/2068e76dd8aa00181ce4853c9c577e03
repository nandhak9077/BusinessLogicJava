import java.util.Scanner;

public class Nine 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.print("Enter the number : ");
		int n = s.nextInt();
		if(n%2==0)
			System.out.println("Even number.");
		else
			System.out.print("Odd number.");
		s.close();
	}
}
